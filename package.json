{
  "name": "zonemaster-gui",
  "version": "0.0.1",
  "license": "MIT",
  "main": "electron/electron-app.js",
  "build": {
    "appId": "zonemaster",
    "app-category-type": "public.app-category.productivity",
    "dmg": {},
    "win": {}
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngsw-config": "node_modules/.bin/ngsw-config dist src/ngsw-config.json",
    "serve:web:prod": "npm run build:web && node dev-server.js",
    "serve:webextension": "npm run build:webextension && web-ext run --source-dir=dist-webextension",
    "build:web": "ng build --prod --aot --app 0 && npm run ngsw-config && cpy node_modules/@angular/service-worker/ngsw-worker.js dist/ && node compress.js",
    "build:webextension": "rimraf ./dist-webextension && ng build --app 2 --aot --prod --sourcemap=false --output-hashing=none && cpy src/manifest-webext.json dist-webextension/ --rename=manifest.json",
    "package:webextension": "npm run build:webextension && web-ext build --source-dir=dist-webextension --artifacts-dir=.",
    "build:wordpress:plugin": "",
    "build:desktop": "rimraf ./dist-electron && ng build --prod --aot --app 1 -e electron --base-href ./ && npm run electron-ntsc && cpy package.json dist-electron",
    "electron-ntsc": "ntsc src/environments/environment.electron.ts --outDir dist-electron/environments",
    "package:desktop": "npm run build:desktop && build --projectDir=dist-electron"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^5.2.1",
    "@angular/common": "^5.2.1",
    "@angular/compiler": "^5.2.1",
    "@angular/core": "^5.2.1",
    "@angular/forms": "^5.2.1",
    "@angular/http": "^5.2.1",
    "@angular/platform-browser": "^5.2.1",
    "@angular/platform-browser-dynamic": "^5.2.1",
    "@angular/router": "^5.2.1",
    "@angular/service-worker": "^5.2.1",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.5",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.0",
    "angular2-markdown": "^2.2.2",
    "bootstrap": "4.0.0-beta.2",
    "core-js": "^2.4.1",
    "electron-builder": "^19.55.2",
    "file-saver": "^1.3.3",
    "font-awesome": "^4.7.0",
    "github-fork-ribbon-css": "^0.2.1",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/core": "0.0.28",
    "@angular-devkit/schematics": "0.0.51",
    "@angular/cli": "^1.6.5",
    "@angular/compiler-cli": "^5.2.1",
    "@angular/language-service": "^5.2.1",
    "@types/file-saver": "^1.3.0",
    "@types/jasmine": "^2.6.2",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^9.3.0",
    "brotli": "^1.3.2",
    "codelyzer": "^4.1.0",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "cpy-cli": "^1.0.1",
    "david": "^11.0.0",
    "electron": "^1.7.9",
    "electron-connect": "^0.6.2",
    "electron-packager": "^10.1.1",
    "electron-reload": "^1.2.2",
    "express-htaccess-middleware": "^0.1.0",
    "express-winston": "^2.4.0",
    "http-server": "^0.11.1",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ntypescript": "^1.201706190042.1",
    "protractor": "^5.2.0",
    "protractor-image-comparison": "^1.3.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3",
    "web-ext": "^2.3.1",
    "web-ext-types": "^2.0.1",
    "winston": "^2.4.0"
  }
}
